{
	"name": "carbonio-mailbox-api-ui",
	"version": "0.0.0",
	"description": "Mailbox API frontend layer",
	"exports": {
		".": {
			"import": "./dist/bundle.mjs",
			"require": "./dist/bundle.cjs",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"clean": "rm -rf dist",
		"build": "npm run build:js && npm run build:types",
		"build:js": "npx rollup -c rollup.config.mjs",
		"build:types": "tsc -p tsconfig.types.json",
		"lint": "eslint --ext .js,.jsx,.ts,.tsx --resolve-plugins-relative-to node_modules/@zextras/carbonio-ui-configs src",
		"prepare": "husky",
		"type-check": "tsc --noEmit",
		"packTo": "npm pack --pack-destination $PKG_PATH && cd $PKG_PATH && npm i carbonio-mailbox-api-ui-$npm_package_version.tgz"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zextras/carbonio-mailbox-api-ui"
	},
	"author": "Zextras Dev Team <https://www.zextras.com/carbonio/>",
	"license": "AGPL-3.0-only",
	"devDependencies": {
		"@babel/core": "^7.26.9",
		"@babel/plugin-transform-runtime": "^7.26.9",
		"@babel/preset-env": "^7.26.9",
		"@babel/preset-react": "^7.26.3",
		"@babel/preset-typescript": "^7.26.0",
		"@commitlint/cli": "^19.6.1",
		"@commitlint/config-conventional": "^19.6.0",
		"@rollup/plugin-babel": "^6.0.4",
		"@rollup/plugin-node-resolve": "^16.0.0",
		"@types/react": "^18.3.7",
		"@zextras/carbonio-ui-configs": "github:zextras/carbonio-ui-configs#feat-split-eslint-config",
		"eslint-plugin-notice": "^1.0.0",
		"husky": "^9.1.7",
		"rollup": "^4.35.0",
		"typescript": "^5.8.2",
		"vitest": "^3.0.8"
	},
	"peerDependencies": {
		"react": "^18.3.1"
	}
}
